        "TEAM_IDS = {\n",
        "    : \"\",\n",
        "    : \"\",\n",
        "    : \"\",\n",
        "    : \"\",\n",
        "    : \"\"\n",
        "}\n",
        "\n",
        "MAP_STATS = pd.DataFrame(\n",
        "    {\n",
        "        \"played\": 0,\n",
        "        \"won\": 0,\n",
        "        \"lost\": 0,\n",
        "        \"total_kills\": 0,\n",
        "        \"total_deaths\": 0,\n",
        "    },\n",
        "    index=[\"de_dust2\", \"de_nuke\", \"de_inferno\", \"de_train\",\n",
        "           \"de_overpass\", \"de_mirage\", \"de_ancient\", \"de_anubis\", \"TOTAL\"]\n",
        ")\n",
        "\n",
        "creds, _ = default()\n",
        "client = gspread.authorize(creds)\n",
        "\n",
        "spreadsheet = client.create(\"cs2 statscraper output\")\n",
        "\n",
        "PATH = \""\n",
        "filenames = os.listdir(PATH)\n",
        "filenames = [f for f in filenames if os.path.isfile(os.path.join(PATH, f))]\n",
        "\n",
        "for file in filenames:\n",
        "  parser = DemoParser(\"//\" + file)\n",
        "\n",
        "  header = parser.parse_header()\n",
        "\n",
        "  wanted_fields = [\"kills_total\", \"deaths_total\", \"assists_total\", \"mvps\", \"headshot_kills_total\", \"ace_rounds_total\", \"4k_rounds_total\", \"3k_rounds_total\", \"score\"]\n",
        "  max_tick = parser.parse_event(\"round_end\")[\"tick\"].max()\n",
        "  scoreboard = parser.parse_ticks(wanted_fields, ticks=[max_tick])\n",
        "  scoreboard[\"name\"] = scoreboard[\"steamid\"].map(TEAM_IDS)\n",
        "  scoreboard = scoreboard.drop(columns=[\"steamid\", \"tick\"])\n",
        "  scoreboard = scoreboard.dropna(subset=[\"name\"])\n",
        "\n",
        "  column_to_move = \"name\"\n",
        "  scoreboard = scoreboard[[column_to_move] + [col for col in scoreboard.columns if col != column_to_move]]\n",
        "  scoreboard = scoreboard.sort_values(by='kills_total', ascending=False)\n",
        "\n",
        "  MAP_STATS.at[header[\"map_name\"], \"played\"] += 1\n",
        "  MAP_STATS.at[header[\"map_name\"], \"total_kills\"] += scoreboard[\"kills_total\"].sum()\n",
        "  MAP_STATS.at[header[\"map_name\"], \"total_deaths\"] += scoreboard[\"deaths_total\"].sum()\n",
        "\n",
        "  print(scoreboard.head())\n",
        "\n",
        "  worksheet = spreadsheet.add_worksheet(title=file, rows=str(len(TEAM_IDS)+1), cols=str(len(wanted_fields)+1))\n",
        "  worksheet.update([[\"Player\", \"Kills\", \"Deaths\", \"Assists\", \"MVPs\", \"Headshot Kills\", \"5k\", \"4k\", \"3k\", \"Score\"]] + scoreboard.reset_index().values.tolist())\n",
        "  set_column_width(worksheet, f'A:{chr(64+len(wanted_fields)+1)}', 150)\n",
        "\n",
        "  df = parser.parse_event(\"player_death\", player=[\"X\", \"Y\", \"team_name\"])\n",
        "\n",
        "  df[\"attacker_steamid\"] = df[\"attacker_steamid\"].fillna(0).astype(\"int64\")\n",
        "  df[\"user_steamid\"] = df[\"user_steamid\"].fillna(0).astype(\"int64\")\n",
        "\n",
        "  for team_id, username in TEAM_IDS.items():\n",
        "    killsOnly = df[[\"attacker_steamid\", \"attacker_X\", \"attacker_Y\", \"attacker_team_name\"]]\n",
        "    deathsOnly = df[[\"user_steamid\", \"user_X\", \"user_Y\", \"user_team_name\"]]\n",
        "\n",
        "    my_kills = killsOnly[killsOnly[\"attacker_steamid\"] == team_id][[\"attacker_X\", \"attacker_Y\", \"attacker_team_name\"]]\n",
        "    my_deaths = deathsOnly[deathsOnly[\"user_steamid\"] == team_id][[\"user_X\", \"user_Y\", \"user_team_name\"]]\n",
        "\n",
        "    my_kills = my_kills.dropna().reset_index(drop=True)\n",
        "    my_deaths = my_deaths.dropna().reset_index(drop=True)\n",
        "\n",
        "    kill_rows = my_kills.values.tolist()\n",
        "    death_rows = my_deaths.values.tolist()\n",
        "\n",
        "    worksheet.append_row([f\"{username}'s kills\"])\n",
        "    worksheet.append_row([\"X\", \"Y\"])\n",
        "    worksheet.append_rows(kill_rows)\n",
        "\n",
        "    worksheet.append_row([f\"{username}'s deaths\"])\n",
        "    worksheet.append_row([\"X\", \"Y\"])\n",
